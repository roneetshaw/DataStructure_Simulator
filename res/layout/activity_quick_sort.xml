<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/qs"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".QuickSort" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="@string/unsort_list"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:text="@string/sort_list"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/num2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/num1"
            android:layout_alignBottom="@+id/num1"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/num1"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/num3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/num2"
            android:layout_alignBottom="@+id/num2"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/num2"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/num4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/num3"
            android:layout_alignBottom="@+id/num3"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/num3"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/sortnum4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/sortnum5"
            android:layout_alignBottom="@+id/sortnum5"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@+id/sortnum5"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/sortnum3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/sortnum4"
            android:layout_alignBottom="@+id/sortnum4"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@+id/sortnum4"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/sortnum2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/sortnum3"
            android:layout_alignBottom="@+id/sortnum3"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@+id/sortnum3"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <Button
            android:id="@+id/btnNext"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/num4"
            android:layout_alignBottom="@+id/num4"
            android:layout_centerHorizontal="true"
            android:background="@drawable/custom_color"
            android:onClick="next"
            android:padding="@dimen/btnStackControl"
            android:text="next"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/sortnum5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/btnNext"
            android:layout_alignParentRight="true"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/num5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/num4"
            android:layout_alignBottom="@+id/num4"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/num4"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/sortnum1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/sortnum2"
            android:layout_alignBottom="@+id/sortnum2"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@+id/sortnum2"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/s4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/s3"
            android:layout_alignBottom="@+id/s3"
            android:layout_toRightOf="@+id/s3"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/s5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/s4"
            android:layout_alignBottom="@+id/s4"
            android:layout_toRightOf="@+id/s4"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/pivtxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imgpiv"
            android:layout_toLeftOf="@+id/imgpiv"
            android:text="Pivot"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/s3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/s2"
            android:layout_alignBottom="@+id/s2"
            android:layout_centerHorizontal="true"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/s2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/s1"
            android:layout_alignBottom="@+id/s1"
            android:layout_toLeftOf="@+id/s3"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/s1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btnNext"
            android:layout_marginTop="25dp"
            android:layout_toLeftOf="@+id/s2"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/f1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/s1"
            android:layout_alignParentBottom="true"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/f2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/f1"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/f3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/f2"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/f4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/f3"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/f5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/f4"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <ImageView
            android:id="@+id/imgpiv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/txtpiv"
            android:layout_marginBottom="5dp"
            android:layout_toLeftOf="@+id/btnNext"
            android:src="@drawable/arr"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/txtpiv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/f3"
            android:layout_marginBottom="21dp"
            android:layout_toLeftOf="@+id/f4"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/num1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/textView1"
            android:layout_marginTop="15dp"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="visible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/t2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/t1"
            android:layout_alignBottom="@+id/t1"
            android:layout_toRightOf="@+id/t1"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/t5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/t4"
            android:layout_alignBottom="@+id/t4"
            android:layout_alignLeft="@+id/s5"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/s1"
            android:layout_below="@+id/s1"
            android:layout_marginTop="15dp"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/t3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/t2"
            android:layout_alignBottom="@+id/t2"
            android:layout_toRightOf="@+id/t2"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/t4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/t3"
            android:layout_alignBottom="@+id/t3"
            android:layout_toRightOf="@+id/t3"
            android:background="@drawable/temp_array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/tvimg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/txtpiv"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="5dp"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/txtcmp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/tvimg"
            android:layout_alignLeft="@+id/txtpiv"
            android:layout_marginBottom="5dp"
            android:background="@drawable/array_node"
            android:gravity="center"
            android:height="@dimen/nodeArrayHeight"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible"
            android:width="@dimen/nodeArray" />

        <TextView
            android:id="@+id/textdesc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/sortnum3"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/s5"
            android:lines="10"
            android:maxLines="10"
            android:text="@string/txtBlank"
            android:textAppearance="?android:attr/textAppearanceLarge" />

    </RelativeLayout>

</RelativeLayout>
