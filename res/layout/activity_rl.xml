<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tree"
    tools:context="com.project.datastructure.RLActivity"
    tools:ignore="MergeRootFrame" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="Right-Left Rotiation (RL) or Double right SetUp"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="40"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageViewLR1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/textView2"
        android:src="@drawable/avlleft" />

    <ImageView
        android:id="@+id/imageView1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_toLeftOf="@+id/textView4"
        android:src="@drawable/avlright" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/textView2"
        android:layout_below="@+id/imageView1_2"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="39"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignRight="@+id/imageViewLR1_1"
        android:layout_below="@+id/imageViewLR1_1"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="38"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageViewRL1"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_alignTop="@+id/textView3"
        android:layout_toRightOf="@+id/progressBarRL1"
        android:src="@drawable/arrownext" />

    <TextView
        android:id="@+id/textViewRL1_1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignTop="@+id/textView4"
        android:layout_toLeftOf="@+id/textView1"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="40"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageViewRL1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewRL1_1"
        android:layout_toLeftOf="@+id/textViewRL1_1"
        android:src="@drawable/avlleft" />

    <TextView
        android:id="@+id/textViewRL1_3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_below="@+id/imageViewRL1_2"
        android:layout_toLeftOf="@+id/imageViewRL1_2"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="38"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textViewRL1_2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_below="@+id/imageViewRL1_1"
        android:layout_toLeftOf="@+id/imageViewRL1_1"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="39"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <ProgressBar
        android:id="@+id/progressBarRL1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textViewRL1_3"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="15dp" />

    <ImageView
        android:id="@+id/imageViewRL2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewRL1_2"
        android:layout_toLeftOf="@+id/textViewRL2_1"
        android:src="@drawable/avlleft" />

    <ImageView
        android:id="@+id/imageViewRL1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewRL1_2"
        android:layout_toLeftOf="@+id/textViewRL1_2"
        android:src="@drawable/avlleft" />

    <TextView
        android:id="@+id/textViewRL2_2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/progressBarRL1"
        android:layout_toLeftOf="@+id/imageViewRL2_1"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="38"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageViewRL2"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_above="@+id/progressBarRL1"
        android:layout_centerHorizontal="true"
        android:src="@drawable/arrowst" />

    <TextView
        android:id="@+id/textViewRL2_1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/imageViewRL2"
        android:layout_alignRight="@+id/textView1"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="39"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageViewRL2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageViewRL2_1"
        android:layout_toRightOf="@+id/textViewRL2_1"
        android:src="@drawable/avlright" />

    <TextView
        android:id="@+id/textViewRL2_3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/progressBarRL1"
        android:layout_toRightOf="@+id/imageViewRL2_2"
        android:background="@drawable/avlnodet"
        android:gravity="center"
        android:text="40"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <ProgressBar
        android:id="@+id/progressBarRL2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/btnRL"
        android:layout_centerVertical="true" />

    <Button
        android:id="@+id/btnRL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageViewLR1_1"
        android:layout_toRightOf="@+id/imageViewRL2_2"
        android:onClick="@string/onClickRL"
        android:text="Rotate(*)" />

    <TextView
        android:id="@+id/textViewRot1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_toRightOf="@+id/imageViewRL1_1"
        android:text="Left Rotation \n around 38"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textViewRot2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textViewRL1_2"
        android:layout_alignRight="@+id/imageViewRL2"
        android:text="Right Rotation \n around 39"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout>